{
  "grid": [
    { "id": "p1", "xs": 6, "class": "main", "table": "units" },
    { "id": "p2", "xs": 6, "class": "main", "table": "units" },
    { "id": "p9", "xs": 12, "class": "main", "table": "unitsTreeTable", "height": "fill" }
  ],

  "spacing": 10,

  "p1": [
    {
      "prop": "header",
      "title": "Сервисный плагин: Отправка sms через gammu",
      "type": "header"
    },

    {
      "prop": "header",
      "title": "Папки из конфигурации gammu:",
      "type": "header"
    },
    {
      "prop": "in_folder",
      "title": "Папка для входящих SMS",
      "type": "input"
    },
    {
      "prop": "out_folder",
      "title": "Папка для исходящих SMS",
      "type": "input"
    },
    {
      "prop": "sent_folder",
      "title": "Папка для отправленных SMS",
      "type": "input"
    },
    {
      "prop": "fail_folder",
      "title": "Папка для не отправленных SMS",
      "type": "input"
    }
  ],

  "p2": [
    {
      "prop": "outtimesec",
      "title": "Время на отправку sms, сек",
      "type": "input",
      "default": 1800
    },
    {
      "prop": "lifetimesec",
      "title": "Время актуальности sms, сек",
      "type": "input",
      "default": 3600
    },
    {
      "prop": "restarttime",
      "title": "$Unit_Restarttime",
      "type": "input",
      "default": 60
    },
    { "prop": "loglevel", "title": "$LogLevel", "type": "droplist", "data": "loglevelList" },
    { "prop": "txt", "title": "$Comment", "type": "input" }
  ],

  "p9": [
    {
      "title": "",
      "type": "table",
      "prop": "unitsTreeTable",

      "columns": [
        { "prop": "name", "title": "ID", "type": "text", "width": 150 },
        { "prop": "state", "title": "Состояние", "type": "text", "width": 150 },
        { "prop": "laststart", "title": "Время запуска", "type": "text", "width": 150 },
        { "prop": "laststop", "title": "Время останова", "type": "text", "width": 150 },
        { "prop": "uptime", "title": "Uptime", "type": "text", "width": 150 },
        { "prop": "error", "title": "Ошибка", "type": "text", "width": 250 }
      ]
    }
  ]
}
